{
  "name": "prettier-hook",
  "version": "0.4.6",
  "author": "Suguru Motegi",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "bin": "./bin/prettier-hook.js",
  "scripts": {
    "build": "tsp build"
  },
  "homepage": "https://github.com/suguru03/prettier-hook",
  "dependencies": {
    "minimist": "^1.2.0",
    "vm-agent": "^0.6.0"
  },
  "peerDependencies": {
    "prettier": ">= 1.13.7"
  },
  "devDependencies": {
    "@types/node": "^10.12.24",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "ts-publisher": "^0.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
