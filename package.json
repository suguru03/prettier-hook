{
  "name": "pre-prettier",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "prettier": "./bin/pre-prettier"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
