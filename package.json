{
  "name": "prettier-hook",
  "version": "0.4.3",
  "author": "Suguru Motegi",
  "license": "MIT",
  "main": "index.js",
  "private": true,
  "bin": "./bin/prettier-hook.js",
  "scripts": {
    "build": "tsp build"
  },
  "homepage": "https://github.com/suguru03/prettier-hook",
  "dependencies": {
    "vm-agent": "^0.5.7"
  },
  "peerDependencies": {
    "prettier": ">= 1.13.7"
  },
  "devDependencies": {
    "@types/node": "^10.12.24",
    "husky": "^0.14.3",
    "lint-staged": "^8.1.5",
    "minimist": "^1.2.0",
    "prettier": "^1.13.7",
    "ts-node": "^8.0.2",
    "ts-publisher": "^0.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
